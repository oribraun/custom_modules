{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "es2020",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "name-entity-recognition": [
        "dist/name-entity-recognition/name-entity-recognition",
        "dist/name-entity-recognition"
      ],
      "ng-trix-lib": [
        "dist/ng-trix-lib"
      ],
      "ng-trix-lib/*": [
        "dist/ng-trix-lib/*"
      ],
      "ng-time-to-string": [
        "dist/ng-time-to-string"
      ],
      "ng-time-to-string/*": [
        "dist/ng-time-to-string/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
